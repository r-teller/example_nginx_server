{
    "type": "object",
    "properties": {
        "request": {
            "type": "object",
            "properties": {
                "uri": {
                    "type": "object",
                    "properties": {
                        "headers": {
                            "type": "object",
                            "additionalProperties": true
                        },
                        "method": {
                            "type": "string",
                            "enum": [
                                "GET",
                                "HEAD",
                                "POST",
                                "PUT",
                                "DELETE",
                                "MKCOL",
                                "COPY",
                                "MOVE",
                                "OPTIONS",
                                "PROPFIND",
                                "PROPPATCH",
                                "LOCK",
                                "UNLOCK",
                                "PATCH"
                            ]
                        },
                        "scheme": {
                            "type": "string"
                        },
                        "path": {
                            "type": "string"
                        },
                        "fullPath": {
                            "type": "string"
                        },
                        "queryString": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "allOf": [
                        {
                            "if": {
                                "properties": {
                                    "method": {
                                        "type": "string",
                                        "enum": [
                                            "POST",
                                            "PUT"
                                        ]
                                    }
                                },
                                "required": [
                                    "method"
                                ]
                            },
                            "then": {
                                "required": [
                                    "body"
                                ]
                            }
                        }
                    ],
                    "required": [
                        "method",
                        "scheme",
                        "path",
                        "fullPath"
                    ]
                },
                "network": {
                    "type": "object",
                    "properties": {
                        "clientPort": {
                            "type": "string"
                        },
                        "clientAddress": {
                            "type": "string"
                        },
                        "serverAddress": {
                            "type": "string"
                        },
                        "serverPort": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "clientPort",
                        "clientAddress",
                        "serverAddress",
                        "serverPort"
                    ]
                },
                "ssl": {
                    "type": "object",
                    "properties": {
                        "isHttps": {
                            "type": "boolean"
                        },
                        "sslProtocol": {
                            "type": "string",
                            "enum": [
                                "SSLv2",
                                "SSLv3",
                                "TLSv1",
                                "TLSv1.1",
                                "TLSv1.2",
                                "TLSv1.3"
                            ]
                        },
                        "sslCipher": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "isHttps"
                    ]
                },
                "session": {
                    "type": "object",
                    "properties": {
                        "requestId": {
                            "type": "string"
                        },
                        "connection": {
                            "type": "string"
                        },
                        "connectionNumber": {
                            "type": "string"
                        },
                        "connectionsActive": {
                            "type": "string"
                        },
                        "connectionsWaiting": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "requestId",
                        "connection",
                        "connectionNumber",
                        "connectionsActive",
                        "connectionsWaiting"
                    ]
                }
            },
            "additionalProperties": false,
            "required": [
                "uri",
                "network",
                "ssl",
                "session"
            ]
        },
        "environment": {
            "type": "object",
            "properties": {
                "metaData": {
                    "type": "object",
                    "properties": {
                        "cloudProvider": {
                            "type": "string",
                            "oneOf": [
                                {
                                    "type": "string",
                                    "const": "gcp"
                                },
                                {
                                    "type": "string",
                                    "const": "unknown"
                                }
                            ]
                        }
                    },
                    "allOf": [
                        {
                            "if": {
                                "properties": {
                                    "cloudProvider": {
                                        "type": "string",
                                        "const": "gcp"
                                    }
                                },
                                "required": [
                                    "cloudProvider"
                                ]
                            },
                            "then": {
                                "properties": {
                                    "zone": {
                                        "type": "string"
                                    },
                                    "image": {
                                        "type": "string"
                                    },
                                    "region": {
                                        "type": "string"
                                    },
                                    "projectId": {
                                        "type": "string"
                                    },
                                    "machineName": {
                                        "type": "string"
                                    },
                                    "machineType": {
                                        "type": "string"
                                    }
                                },
                                "propertyNames": {
                                    "enum": [
                                        "cloudProvider",
                                        "zone",
                                        "image",
                                        "region",
                                        "projectId",
                                        "machineName",
                                        "machineType"
                                    ]
                                }
                            }
                        }
                    ]
                },
                "hostname": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "hostname"
            ]
        },
        "response": {
            "type": "object",
            "properties": {
                "addedDelay": {
                    "type": "object",
                    "properties": {
                        "status": {
                            "type": "string",
                            "enum": [
                                "ENABLED",
                                "DISABLED"
                            ]
                        },
                        "min": {
                            "type": "integer"
                        },
                        "max": {
                            "type": "integer"
                        },
                        "actual": {
                            "type": "integer"
                        },
                        "timeStamp": {
                            "type": "string"
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "status"
                    ]
                },
                "body": {
                    "type": "string"
                },
                "statusCode": {
                    "type": "integer",
                    "minimum": 100,
                    "maximum": 999
                },
                "statusReason": {
                    "type": "string",
                    "enum": [
                        "DEFAULT",
                        "RANDOM_SERVER_PORT_RANGE",
                        "QUERY_PARAM_RANDOM_FAIL_RATE",
                        "QUERY_PARAM_STATUS",
                        "UNHEALTHY_SERVER_PORT_RANGE"
                    ]
                },
                "timeStamp": {
                    "type": "string"
                }
            },
            "allOf": [
                {
                    "if": {
                        "properties": {
                            "statusCode": {
                                "type": "integer",
                                "minimum": 600
                            }
                        },
                        "required": [
                            "statusCode"
                        ]
                    },
                    "then": {
                        "properties": {
                            "statusBody": {
                                "type": "string",
                                "pattern": "^UNKNOWN_QUERY_PARAM_STATUS - [0-9]{3}"
                            }
                        }
                    },
                    "else": {
                        "properties": {
                            "statusBody": {
                                "type": "string"
                            }
                        }
                    }
                }
            ],
            "propertyNames": {
                "enum": [
                    "body",
                    "addedDelay",
                    "statusBody",
                    "statusCode",
                    "statusReason",
                    "timeStamp"
                ]
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "request",
        "environment",
        "response"
    ]
}